{
    "friendlyname": "bminer",
    "version": "$bminer_ver",
    "hcd": "bminer -uri ${!xproto}://${!xaddr}${!xwallet}${!xwork}:$MINER_PWD@${!xpool}:${!xport} ${!xext} $BMINER_OPTS ",
    "devlist_format": 0,

    "minerinfo":
    {
      "total_hash": "tail -100 ${NVOC}/nvoc_logs/screenlog.0 | grep -o "Total [0-9]*" | awk '{print $2}' | awk '{s+=$1} END {printf "%.0f", s}'
",
      "hash_count": "tail -100 ${NVOC}/nvoc_logs/screenlog.0 | grep -o "Total [0-9]*" | awk '{print $2}' | grep -v grep | wc -l | awk '{print $1}'
",
      "solution_speed_unit": "Mh/s",
      "miner_error": "false",
      "rejected_shares": "false"
    },

    "install":
    {
      "latest": "true",
      "recommanded": "cuda-9.2",
      "tarball": "$bminer_tarball_ver",
      "executable": "bminer"
    },

    "compile":
    {
      "src_path": "false",
      "src_commit_hash": "",
      "command": "",
      "output": ""
    }
}
