    },

    "install":
    {
      "latest": "true",
      "recommanded": "cuda-9.2",
      "tarball": "$claymore_tarball_ver",
      "executable": "claymore"
    },

    "compile":
    {
      "src_path": "false",
      "src_commit_hash": "",
      "command": "",
      "output": ""
    }
}
